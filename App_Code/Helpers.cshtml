@helper Tags(System.Web.Mvc.HtmlHelper htmlHelper,
    IList<KatInTheCodeSite.Core.Objects.Tag> tags)
{
                    /**/

                    foreach (var tag in tags)
                    {
                        <div class="tags-div">
                            @KatInTheCodeSite.ActionLinkExtensions.TagLink(htmlHelper, tag)
                        </div>
                    }
}